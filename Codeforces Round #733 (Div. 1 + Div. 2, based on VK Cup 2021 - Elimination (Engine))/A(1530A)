#include<bits/stdc++.h>
using namespace std;

int main(){
    long long a;
    cin>>a;
    for (long long i = 0; i < a; i++)
    {
        string s;
        cin>>s;
        long long maxa=0;
        for (long long j = 0; j < s.size(); j++)
        {
            maxa=max(maxa,(long long)s[j]-'0');
        }
        cout<<maxa<<endl;
    }
    
}
